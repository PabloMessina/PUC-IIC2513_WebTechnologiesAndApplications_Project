<%= form_for(@grocery, url: @namespace) do |f| %>

 	<%= render 'shared/error_messages', object: @grocery %>

 	<div class = "field">
    <%= f.label :image, "Image" %>
    <%= f.file_field :image %>
  </div>

	<div class = "field">
		<%= f.label :name, "Name" %>
		<%= f.text_field :name %>
	</div>

	<div class = "field">
		<%= f.label :address, "Address" %>
		<%= f.text_field :address %>
	</div>

	<%= f.submit @submit_message%>
<% end %>
